{# Add action template for Ads controller #}
{% extends 'base.twig' %}

{% block title %}{{  'Add ad'|trans }}{% endblock %}

{% block content %}
    <h2 class="text-center">
        {{ 'Add ad'|trans }}
    </h2>





    <form  action="{{ url('ads_add') }}" method="post" class="form-inline"  >
       {{ form_widget(form ) }}
        <input type="submit" name="submit" class="btn btn-primary" value="{{ 'Save'|trans }}" />
    </form>
{% endblock %}

