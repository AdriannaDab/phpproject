{# Register data action template for Users controller #}
{% extends 'base.twig' %}
{% block title %}{{ 'Add new data'|trans }}{% endblock %}
{% block content %}
    <hr>
    <h2 class="intro-text text-center">
        {{ 'Add new data'|trans }}
    </h2>
    <hr>
    <div class="col-md-3"></div>
    <div class="col-md-6">
    <form role="form" action="" method="post">
        {{ form_start(form)  }}
        <div class="form-group">
            {{ form_row(form.city_name) }}
        </div>
        <input type="submit" name="submit" class="btn btn-primary" value="{{ 'Add'|trans }}" />
        <a href="{{ url('users_data') }}" class="btn btn-default">{{ 'Cancel'|trans }}</a>

        {{ form_end(form) }}

    </form>
    </div>
    <br>


{% endblock %}