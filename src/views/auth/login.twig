{% extends 'base.twig' %}

{% block title %}{{ 'Log in'|trans }}{% endblock %}

{% block content %}
    <h1>
        {{ 'Log in'|trans }}
    </h1>
    {{ error }}
    <form action="{{ path('auth_login_check') }}" method="post">
        {{ form_widget(form) }}
        <input type="submit" name="submit" class="btn btn-primary" value="{{ 'Log in'|trans }}" />
        <a href="{{ url('/ads/') }}" class="btn btn-default">{{ 'Cancel'|trans }}</a>
    </form>
{% endblock %}

